{
  "env": {
    "browser": true,
    "jasmine": true,
    "angular/mocks" :  true
  },
  "plugins": [ "angular", "jasmine" ],
  "globals": {
    "moment": true,
    "saveAs":  true 
  },
  "extends": [
    "eslint:recommended",
    "angular",
    "plugin:jasmine/recommended"
  ],
  "rules": {
    "quotes": [ 2, "single" ],
    "angular/on-watch": "warn",
    "angular/di": [ 2, "array" ]
  }
}
